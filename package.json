    "react-redux": "^4.4.5",
    "redux": "^3.5.1",
    "redux-logger": "^2.6.1",
    "redux-persist": "^3.0.3",
    "redux-thunk": "^2.0.1",
    "remote-redux-devtools": "^0.5.3"
